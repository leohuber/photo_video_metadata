#!/bin/bash

meta_file="zzz_meta_data_template.txt"

rm -f $meta_file &> /dev/null
touch $meta_file
echo "# Generated meta data template file" >> $meta_file
echo "" >> $meta_file
echo "MAKE='Canon'" >> $meta_file
echo "MODEL='Canon EOS R6'" >> $meta_file
echo "GPS='-35.2975906, 149.1012676, 554'" >> $meta_file
echo "" >> $meta_file
echo "CREATOR='Leo Huber'" >> $meta_file
echo "COUNTRY_CODE='CHE'" >> $meta_file
echo "COUNTRY='Switzerland'" >> $meta_file
echo "STATE='Zurich'" >> $meta_file
echo "CITY='Zurich'" >> $meta_file
echo "SUBLOCATION='Sublocation'" >> $meta_file
echo "" >> $meta_file
echo "HEADLINE='Headline'" >> $meta_file
echo "TITLE_SUFFIX='title_suffix'" >> $meta_file
echo "DESCRIPTION='Description'" >> $meta_file
echo "COPYRIGHT='Leo Huber'" >> $meta_file
